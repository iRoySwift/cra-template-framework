        "eject": "react-scripts eject",
        "eslint:fix": "eslint --fix .",
        "lint:css": "stylelint src/**/*.tsx"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint && yarn lint:css"
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "plugin:prettier/recommended"

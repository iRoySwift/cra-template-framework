        "i18next": "^22.0.4",
        "react": "^18.0.0",
        "react-app-alias": "^2.2.2",
        "react-device-detect": "^2.2.2",
        "react-dom": "^18.0.0",
        "react-i18next": "^12.0.0",
        "react-redux": "^8.0.5",
